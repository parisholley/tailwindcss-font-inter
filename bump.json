{
    "changelog": {
        "enabled": true,
        "create": false,
        "open": true,
        "file": "CHANGELOG.md",
        "version": "### Version [version]\n",
        "commit": "- [message]",
        "separator": "\n"
    },
    "tag": {
        "enabled": false,
        "name": "v[version]"
    },
    "release": {
        "github": {
          "enabled": true,
          "files": [
            "src/**",
            "inter.json"
          ]
        }
      },
    "scripts": {
        "postrelease": "npm publish"
    }
}
