{
    "changelog": {
        "enabled": true,
        "create": false,
        "open": true,
        "file": "CHANGELOG.md",
        "version": "### Version [version]\n",
        "commit": "- [message]",
        "separator": "\n"
    },
    "tag": {
        "enabled": true,
        "name": "v[version]"
    },
    "scripts": {
        "posttag": "npm publish"
    }
}
